{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "Domain Redirect": {
            "matchCondition": {
                "route": "/{shortUrl}",
                "methods": ["GET"]
            },
            "backendUri": "http://%WEBSITE_HOSTNAME%/api/Redirect/{shortUrl}"
        }
    }
}