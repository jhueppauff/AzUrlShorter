{
    "bindings": [{
            "route": "redirect/{shortUrl}",
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": [
                "get"
            ]
        },
        {
            "type": "http",
            "direction": "out",
            "name": "res"
        },
        {
            "name": "tableInput",
            "type": "table",
            "tableName": "shorturls",
            "connection": "AzureStorageConnection",
            "direction": "in",
            "filter": "(PartitionKey eq '{shortUrl}')"
        }
    ],
    "scriptFile": "../dist/Redirect/index.js"
}