{
  "bindings": [
    {
      "route": "redirect/{shortUrl}",
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name" : "shortUrl",
      "type": "table",
      "tableName": "shorturls",
      "connection": "AzureStorageConnection",
      "direction": "in",
      "partitionKey": "{shortUrl}",
      "rowKey": "1"
    }
  ],
  "scriptFile": "../dist/Redirect/index.js"
}
